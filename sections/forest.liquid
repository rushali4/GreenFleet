{{ 'forestMap.css' | asset_url | stylesheet_tag }}
<script src="{{ 'forestGoogleMap.js' | asset_url }}" defer="defer"></script>
<div>
  <h1>{{ section.settings.headline }}</h1>
  
  {% for block in section.blocks %}
    <img src ={{ block.settings.image_1 |image_url: width:50, height:50}} alt=""
        width="100"
        height = "100"/>

  {% endfor %}
</div>
