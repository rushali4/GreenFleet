
<div width: 100% height: 450px>
  <h1>{{ section.settings.headline }}</h1>
  <img src ={{ section.blocks[0].settings.image_1 |image_url: width:50, height:50}} alt=""
        width="100"
        height = "100"/>
  {% for product in section.blocks %}
    <img src ={{ section.blocks.settings.id |image_url: width:50, height:50}} alt=""
        width="100"
        height = "100"/>

  {% endfor %}
</div>

{% schema %}
  {
    "name": "Forest",
    "settings": [
      {
        "type": "text",
        "label": "Headline",
        "id" : "headline"
      }
    ],
    "blocks": [
      {
        "name" : "image",
        "type": "image",
        "settings": [
          {
            "type": "image_picker",
            "id": "image_1",
            "label": "image"
          }
        ]
      }
    ],
    "presets": [{
        "name": "Forest"
      }
    ]
  }
{% endschema %}