{% schema %}
  {
    "name": "Forest",
    "settings": [
      {
        "type": "text",
        "label": "Headline",
        "id" : "headline"
      }
    ],
    "blocks": [
      {
        "name" : "image",
        "type": "image",
        "settings": [
          {
            "type": "image_picker",
            "id": "image_1",
            "label": "image"
          }
        ]
      }
    ],
    "presets": [{
        "name": "Forest"
      }
    ]
  }
{% endschema %}