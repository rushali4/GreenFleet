
<div width: 100% height: 450px>
  <h1>{{ section.settings.headline }}</h1>
  <img src ={{ section.blocks.settings.image |img_url: 'master'}} 
        height="100" 
        width="100">
</div>

{% schema %}
  {
    "name": "Forest",
    "settings": [
      {
        "type": "text",
        "label": "Headline",
        "id" : "headline"
      }
    ],
    "blocks": [
      {
        "name" : "image",
        "type": "image",
        "settings": [
          {
            "type": "image_picker",
            "id": "image",
            "label": "image"
          }
        ]
      }
    ],
    "presets": [{
        "name": "Forest"
      }
    ]
  }
{% endschema %}